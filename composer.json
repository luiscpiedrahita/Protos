{
  "name": "Protos",
  "type": "wordpress-theme",
  "license": "MIT",
  "description": "WordPress theme based on Roots.io Sage",
  "homepage": "https://lmseo.co",
  "authors": [
    {
      "name": "Luis Piedrahita",
      "email": "luis@lmseo.co",
      "homepage": "https://github.com/luiscpiedrahita"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/luiscpiedrahita/Protos/issues"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=8.2",
    "log1x/sage-directives": "^2.0",
    "log1x/sage-svg": "^2.0",
    "roots/acorn": "^5.0.5"
  },
  "require-dev": {
    "laravel/pint": "^1.27"
  },
  "suggest": {
    "log1x/sage-directives": "A collection of useful Blade directives for WordPress and Sage (^1.0)."
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
